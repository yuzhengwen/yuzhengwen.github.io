---
import { ClientRouter } from "astro:transitions";
import NavBar from "../components/NavBar.astro";
import "../styles/main.css";
import { baseUrl } from "../resolveBaseUrl";
console.log(import.meta.env.BASE_URL);
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href=`${baseUrl}/favicon.ico` />
    <meta name="generator" content={Astro.generator} />
    <title>Yu Zhengwen</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />

    <script
      async
      is:inline
      data-astro-rerun
      defer
      src="https://cloud.umami.is/script.js"
      data-website-id="a3ba7510-3f85-4380-b244-9ebc4fa681ab"></script>

    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"></script>
    <ClientRouter />
  </head>
  <body>
    <NavBar />
    <div class="main-container">
      <slot />
    </div>
  </body><script src="../scripts/mouseClick.ts"></script>
</html>
